{
  "name": "kia-tere-server",
  "version": "1.0.0",
  "description": "WebSocket server for Kia Tere multiplayer word game",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:watch": "nodemon --exec ts-node src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint:check": "eslint src --ext .ts --max-warnings 0",
    "lint:fix": "eslint src --ext .ts --fix",
    "format:check": "prettier --check src",
    "format:fix": "prettier --write src",
    "check": "npm run lint:check && npm run format:check",
    "fix": "npm run lint:fix && npm run format:fix"
  },
  "dependencies": {
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.4",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "websocket",
    "game",
    "multiplayer",
    "kia-tere",
    "maori",
    "typescript"
  ],
  "author": "",
  "license": "MIT"
}
